{
    "name": "terminal42/contao-folderpage",
    "type": "contao-bundle",
    "description": "Contao Open Source CMS extension folderpage",
    "keywords": ["contao", "folder", "page", "folderpage"],
    "license": "LGPL-3.0+",
    "authors":[
        {
            "name":"terminal42 gmbh",
            "homepage":"http://terminal42.ch"
        }
    ],
    "require": {
        "php": "^5.4 || ^7.0",
        "contao/core-bundle": "^4.1.3"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.1"
    },
    "replace": {
        "contao-legacy/folderpage": "self.version"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Terminal42\\FolderpageBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Terminal42\\FolderpageBundle\\ContaoManager\\Plugin"
    }
}
